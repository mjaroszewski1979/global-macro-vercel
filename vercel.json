{
    "version": 2,
    "builds": [
      {
        "src": "scraper/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.8" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "scraper/wsgi.py"
      }
    ]
  }